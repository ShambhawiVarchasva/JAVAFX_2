<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="800.0" prefWidth="1000.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="application.registerController">
   <center>
      <AnchorPane prefHeight="800.0" prefWidth="1000.0" style="-fx-background-color: #96a39d;" BorderPane.alignment="CENTER">
         <children>
            <Label id="welcome-text" layoutX="290.0" layoutY="99.0" prefHeight="67.0" prefWidth="363.0" stylesheets="@application.css" text="Registration Form" />
            <TextField id="text" fx:id="name" layoutX="323.0" layoutY="217.0" prefHeight="61.0" prefWidth="258.0" promptText="Name" stylesheets="@application.css" />
            <TextField id="text" fx:id="username" layoutX="323.0" layoutY="309.0" prefHeight="52.0" prefWidth="261.0" promptText="username" stylesheets="@application.css" />
            <PasswordField id="text" fx:id="password" layoutX="320.0" layoutY="496.0" prefHeight="52.0" prefWidth="264.0" promptText="Password" stylesheets="@application.css" />
            <PasswordField id="text" fx:id="confirm" layoutX="316.0" layoutY="587.0" prefHeight="55.0" prefWidth="265.0" promptText="Confirm Password" stylesheets="@application.css" />
            <TextField id="text" fx:id="email" layoutX="322.0" layoutY="400.0" prefHeight="52.0" prefWidth="261.0" promptText="Email" stylesheets="@application.css" />
            <Button layoutX="328.0" layoutY="705.0" mnemonicParsing="false" onAction="#signUp" prefHeight="32.0" prefWidth="245.0" text="Sign Up" />
            <Label fx:id="message" layoutX="706.0" layoutY="400.0" prefHeight="77.0" prefWidth="188.0" />
          
            <Button fx:id="goBackToLogin" layoutX="84.0" layoutY="117.0" mnemonicParsing="false" onAction="#goBackToLogin" prefHeight="32.0" prefWidth="149.0" text="Go back to Login" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>


